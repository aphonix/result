{
  "name": "aphonix/result",
  "description": "A zero-dependency, production-ready PHP implementation of Rust's Result type for elegant error handling.",
  "keywords": [
    "rust",
    "result",
    "error-handling",
    "functional",
    "monad",
    "option",
    "safe"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "aphonix",
      "email": "bqdove@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/aphonix/result/issues",
    "source": "https://github.com/aphonix/result"
  },
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Aphonix\\Result\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Aphonix\\Result\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}